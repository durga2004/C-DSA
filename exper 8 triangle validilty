#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Enter three angles: ";
    cin >> a >> b >> c;
    if (a + b + c == 180 && a > 0 && b > 0 && c > 0)
        cout << "The angles form a valid triangle." << endl;
    else
        cout << "The angles do not form a valid triangle." << endl;
    return 0;
}
